<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>A reusable DNS resolver for .NET.</Description>
    <TargetFramework>netstandard2.1</TargetFramework>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <Version>2.1.0</Version>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackOnBuild>true</PackOnBuild>
    <Authors>Alphons van der Heijden, Geoffrey Huntley</Authors>
    <PackageId></PackageId>
    <PackageVersion>2.1.0</PackageVersion>
    <PackageProjectUrl>https://github.com/ghuntley/Heijden.Dns</PackageProjectUrl>
    <PackageLicense>LICENSE</PackageLicense>
    <Title>SpeedTest.Net</Title>
    <LangVersion>8</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="..\..\LICENSE.md">
      <Link>LICENSE</Link>
    </Content>
  </ItemGroup>
</Project>

